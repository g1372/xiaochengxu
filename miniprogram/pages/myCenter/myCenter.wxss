/**index.wxss**/
page {
  background: #f8f8f8;
}

.container {
  width: 100%;
  height: 450rpx;
  margin-bottom: 30rpx;
  position: relative;
}

.container>image {
  width: 100%;
  height: 100%;
  position: absolute;
  z-index: -1;
}

.userLogin {
  width: 100%;
  height: 150rpx;
  margin-top: 100rpx;
  display: flex;
  align-items: center;
}

.userLogin_img {
  margin-left: 50rpx;
  width: 150rpx;
  height: 100%;
}

.userLogin_img image {
  width: 100%;
  height: 100%;
  border-radius: 50%;
}

.userLogin_name {
  margin-left: 30rpx;
  width: 40%;
  color: rgb(94, 94, 94);
}

.userLogin_name view:nth-of-type(1) {
  padding-left: 5rpx;
}

.userLogin_name view:nth-of-type(2) {
  margin-top: 10rpx;
  width: 70rpx;
  padding: 4rpx;
  /* background-color: rgba(61, 61, 61, 0.3); */
  border-radius: 50rpx;
  text-align: center;
}

.userLogin_btn {
  height: 60rpx;
  width: 150rpx;
  color: rgb(94, 94, 94);
  border: 1px solid rgb(94, 94, 94);
  border-radius: 10rpx;
  line-height: 60rpx;
  text-align: center;
}

.integral {
  width: 100%;
  display: flex;
  justify-content: space-around;
  margin-top: 50rpx;
}

.integral_list {
  width: 20%;
  text-align: center;
  color: rgb(94, 94, 94);
  font-size: 32rpx;
}

.integral_list>view {
  margin-top: 20rpx;
}

.module {
  /* width: 95%; */
  margin: auto;
}

.module_list {
  background-color: #fff;
  height: 100rpx;
  padding-left: 30rpx;
  margin-bottom: 10rpx;
  color: rgb(83, 83, 83);
  font-size: 32rpx;
  line-height: 100rpx;
}

.special {
  margin-bottom: 40rpx;
}

.ztright{
  float: right;
  margin-right: 50rpx;
}

